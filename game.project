[project]
title = HoppingBob
version = 0.1
dependencies = https://github.com/britzl/defold-timer/archive/master.zip

[bootstrap]
main_collection = /main/main.collectionc
render = /src/render/render.renderc

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[display]
width = 720
height = 1280
variable_dt = 1

[physics]
scale = 0.02
debug = 0

[script]
shared_state = 1

[profiler]
track_cpu = 0

